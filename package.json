{
  "name": "@acrontum/moxy",
  "version": "0.0.1",
  "description": "Simple, configurable part mock part proxy",
  "main": "dist/src/index.js",
  "scripts": {
    "setup": "git config core.hooksPath ./githooks",
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint '{src,test}/**/*.{js,ts,json}'",
    "watch": "npx nodemon --ext '*' --delay 500ms --watch 'src' --watch 'overrides' --watch 'static' --exec 'npm run build && npm start'",
    "test": "NODE_ENV=test NODE_ENV=test mocha --require source-map-support/register --recursive 'dist/test/**/*.spec.js'"
  },
  "bin": {
    "moxy": "dist/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrontum/moxy.git"
  },
  "keywords": [
    "mock",
    "proxy",
    "api"
  ],
  "author": "p-mcgowan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acrontum/moxy/issues"
  },
  "homepage": "https://github.com/acrontum/moxy#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.10.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "chai": "^4.3.6",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@tsconfig/node16": "^1.0.2"
  }
}
